ияние.","translationFull":["BLAST"],"mangaka":["Ima Ichiko"],"transStatus":"Глав: 5, перевод завершен","status":"Томов: 1, выпуск завершен","middleUrl":"manga","img":"/showfull/uploads/posts/2012-08/thumbs/1343846549_i75151.jpg","altName":"the-paradise-on-the-sand","rating":15,"title":"Рай на песках (The Paradise on the Sand)","apiId":15612,"__v":0}